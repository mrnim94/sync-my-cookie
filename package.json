    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "antd": "^5.10.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-import": "^1.13.8",
    "babel-plugin-lodash": "^3.3.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "fs-extra": "^11.1.1",
    "html-webpack-plugin": "^5.5.3",
